{
  "name": "ember-semantic-ui-table",
  "description": "Semantic UI Table component for Ember.",
  "url": "http://github.com/quantosobra/ember-semantic-ui-table",
  "version": "0.0.0",
  "options": {
    "enabledEnvironments": ["production"],
    "themedir" : "node_modules/yuidoc-theme-quantosobra",
    "paths": [
      "addon"
    ],
    "external": {
      "data": [{
        "base": "http://emberjs.com/api/",
        "json": "http://builds.emberjs.com/tags/v2.4.0/ember-docs.json"
      }]
    },
    "exclude": "vendor",
    "outdir": "docs",
    "linkNatives": true,
    "quiet": true,
    "parseOnly": false,
    "lint": false,
    "nocode": false
  }
}
